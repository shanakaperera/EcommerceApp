<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.certus.dbmodel.TermsConditions" table="terms_conditions" catalog="shopping_store" optimistic-lock="version">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="mainSec" type="string">
            <column name="main_sec" length="65535" />
        </property>
        <property name="securitySec" type="string">
            <column name="security_sec" length="65535" />
        </property>
        <property name="serviceOffered" type="string">
            <column name="service_offered" length="65535" />
        </property>
        <property name="privacyPolicy" type="string">
            <column name="privacy_policy" length="65535" />
        </property>
        <property name="usrCnductRules" type="string">
            <column name="usr_cnduct_rules" length="65535" />
        </property>
        <property name="compensationPolicy" type="string">
            <column name="compensation_policy" length="65535" />
        </property>
        <property name="extnessGrnteed" type="string">
            <column name="extness_grnteed" length="65535" />
        </property>
        <property name="limitationOfLiability" type="string">
            <column name="limitation_of_liability" length="65535" />
        </property>
        <property name="pricing" type="string">
            <column name="pricing" length="65535" />
        </property>
        <property name="delivery" type="string">
            <column name="delivery" length="65535" />
        </property>
        <property name="serviceFees" type="string">
            <column name="service_fees" length="65535" />
        </property>
    </class>
</hibernate-mapping>
